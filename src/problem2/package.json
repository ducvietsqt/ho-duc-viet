  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "env-cmd -f ./env/.env.development react-scripts start",
    "start:stag": "env-cmd -f ./env/.env.staging react-scripts start",
    "start:prod": "env-cmd -f ./env/.env.production react-scripts start",
    "build:dev": "env-cmd -f ./env/.env.development react-scripts build",
    "build:stag": "env-cmd -f ./env/.env.staging react-scripts build",
    "build:prod": "env-cmd -f ./env/.env.production react-scripts build",
    "check-node-version": "node check-node-version.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --max-warnings=0 .",
    "format": "prettier --write \"**/*.+(js|ts||jsx|tsx|json|yml|yaml|md|html)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4",
    "@types/react-select": "^5.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3"
  }
